{
  "name": "hide-your-mess-behind",
  "version": "1.1.0",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.example.hideyourmessbehind",
    "mac": {
      "icon": "assets/icons/icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icons/icon.ico"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "zip"
      ],
      "icon": "assets/icons/icon.png",
      "category": "Utility"
    },
    "files": [
      "src/**/*.js",
      "src/index.html",
      "src/styles.css",
      "package.json",
      "models/**",
      "assets/**/*"
    ],
    "asarUnpack": [
      "models/**",
      "assets/openvino-logo.png"
    ]
  },
  "devDependencies": {
    "electron": "^32.1.0",
    "electron-builder": "^25.0.5"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "openvino-node": "2024.3.0",
    "sharp": "^0.33.5",
    "@napi-rs/canvas": "^0.1.52"
  },
  "keywords": [],
  "author": "Miko≈Çaj Roszczyk <mikolaj.roszczyk@intel.com>",
  "contributors": [
    {
      "name": "Adrian Boguszewski"
    }
  ],
  "homepage": "https://github.com/openvinotoolkit/openvino_build_deploy",
  "license": "Apache-2.0",
  "description": "An application to present background blurring with OpenVINO"
}
